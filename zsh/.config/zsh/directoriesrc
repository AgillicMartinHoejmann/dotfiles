alias ..="cd .."
alias ...="cd ../.."
alias cac="cd ${XDG_CACHE_HOME:-$HOME/.cache}"
alias cf="cd ${XDG_CONFIG_HOME:-$HOME/.config}"
alias D="cd ${XDG_DOWNLOAD_DIR:-$HOME/Downloads}"
alias d="cd ${XDG_DOCUMENTS_DIR:-$HOME/Documents}"
alias dt="cd ${XDG_DATA_HOME:-$HOME/.local/share}"
alias h="cd ${HOME}"
alias mn="cd /mnt"
